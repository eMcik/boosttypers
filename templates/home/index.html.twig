{% extends 'base.html.twig' %}

{% block title %}Deers gallery{% endblock %}

{% block body %}
    <h3>Deers Galleries</h3>
    <div>
        <ul>
        {% for gallery in galleries %}
            <li><a href="{{ path('gallery', {id: gallery.id}) }}">{{ gallery.name }}</a></li>
        {% endfor %}
        </ul>
    </div>
{% endblock %}